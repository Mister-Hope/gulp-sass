{
  "name": "@mr-hope/gulp-sass",
  "version": "2.0.0",
  "description": "Gulp plugin for sass",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b tsconfig.project.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mister-Hope/gulp-sass"
  },
  "keywords": [
    "gulpplugin",
    "sass",
    "gulp"
  ],
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/gulp-sass/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/sass": "^1.16.0",
    "chalk": "^4.1.0",
    "lodash": "^4.17.21",
    "plugin-error": "^1.0.1",
    "replace-ext": "^2.0.0",
    "sass": "^1.32.8",
    "strip-ansi": "^6.0.0",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "@types/globule": "1.1.3",
    "@types/gulp": "4.0.8",
    "@types/gulp-postcss": "8.0.0",
    "@types/gulp-sourcemaps": "0.0.34",
    "@types/gulp-tap": "^1.0.0",
    "@types/jest": "26.0.22",
    "@types/rimraf": "3.0.0",
    "@types/vinyl": "2.0.4",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "autoprefixer": "10.2.5",
    "conventional-changelog-cli": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "del": "6.0.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "globule": "1.3.2",
    "gulp": "4.0.2",
    "gulp-postcss": "9.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-tap": "2.0.0",
    "jest": "27.0.0-next.6",
    "postcss": "8.2.12",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.0-next.10",
    "typescript": "4.2.3",
    "vinyl": "2.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
