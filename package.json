{
  "name": "gulp5-sass-plugin",
  "version": "1.0.0",
  "description": "Gulp plugin for sass",
  "type": "module",
  "keywords": [
    "gulp-plugin",
    "sass",
    "gulp"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/gulp5-sass-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mister-Hope/gulp5-sass-plugin"
  },
  "license": "MIT",
  "author": "Mr.Hope",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "changelog": "standard-version --skip.bump --skip.commit --skip.tag",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "packages:check-update": "pnpm dlx npm-check-updates -u --timeout 600000",
    "packages:update": "pnpm up -r",
    "test": "vitest run --coverage"
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,yaml,yml,md}": "prettier --write",
    "*.wxss": "stylelint --fix",
    "*.wxml": "prettier --parser html --write",
    "*.wxs": "prettier --parser babel-flow --write",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "picocolors": "^1.0.0",
    "plugin-error": "^2.0.1",
    "replace-ext": "^2.0.0",
    "sass": "^1.72.0",
    "source-map-js": "^1.2.0",
    "strip-ansi": "^7.1.0",
    "vinyl": "^3.0.0",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "peerDependencies": {
    "gulp": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@types/globule": "1.1.9",
    "@types/gulp": "4.0.17",
    "@types/gulp-postcss": "8.0.6",
    "@types/gulp-sourcemaps": "0.0.38",
    "@types/gulp-tap": "1.0.5",
    "@types/node": "^20.12.1",
    "@types/vinyl": "2.0.11",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@vitest/coverage-v8": "1.4.0",
    "autoprefixer": "10.4.19",
    "commitizen": "4.3.0",
    "cz-git": "1.9.0",
    "del": "7.1.0",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "globule": "1.3.4",
    "gulp": "5.0.0",
    "gulp-postcss": "10.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-tap": "2.0.0",
    "husky": "9.0.11",
    "nano-staged": "0.8.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "rollup": "4.13.2",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.1",
    "standard-version": "9.5.0",
    "typescript": "5.4.3",
    "vite": "5.2.7",
    "vitest": "1.4.0"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@8.15.5",
  "publishConfig": {
    "access": "public"
  }
}
