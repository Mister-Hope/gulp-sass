{
  "name": "@mr-hope/gulp-sass",
  "version": "3.1.1",
  "description": "Gulp plugin for sass",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "changelog": "standard-version --skip.bump --skip.commit --skip.tag",
    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "packages:check-update": "pnpm dlx npm-check-updates -u --timeout 600000",
    "packages:update": "pnpm up -r",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mister-Hope/gulp-sass"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gulp-plugin",
    "sass",
    "gulp"
  ],
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/gulp-sass/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,yaml,yml,md}": "prettier --write",
    "*.wxss": "stylelint --fix",
    "*.wxml": "prettier --parser html --write",
    "*.wxs": "prettier --parser babel-flow --write",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@types/node": "^20.5.6",
    "picocolors": "^1.0.0",
    "plugin-error": "^2.0.1",
    "replace-ext": "^2.0.0",
    "sass": ">= 1.45.0",
    "source-map-js": "^1.0.2",
    "strip-ansi": "^6.0.1",
    "vinyl": "^2.2.1",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@types/globule": "1.1.6",
    "@types/gulp": "4.0.13",
    "@types/gulp-postcss": "8.0.3",
    "@types/gulp-sourcemaps": "0.0.36",
    "@types/gulp-tap": "1.0.2",
    "@types/rimraf": "3.0.2",
    "@types/vinyl": "2.0.7",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitest/coverage-v8": "0.34.2",
    "autoprefixer": "10.4.15",
    "commitizen": "4.3.0",
    "cz-git": "1.7.1",
    "del": "7.1.0",
    "esbuild": "0.19.2",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "globule": "1.3.4",
    "gulp": "4.0.2",
    "gulp-postcss": "9.0.1",
    "gulp-sourcemaps": "3.0.0",
    "gulp-tap": "2.0.0",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "postcss": "8.4.29",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "rollup": "3.29.1",
    "rollup-plugin-dts": "6.0.2",
    "rollup-plugin-esbuild": "5.0.0",
    "standard-version": "9.5.0",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.2"
  },
  "packageManager": "pnpm@8.7.5",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "chokidar": "^3.5.3",
      "css": "npm:@adobe/css-tools@^4.3.1",
      "glob-stream": "^7.0.0",
      "glob-parent": "^6.0.2",
      "micromatch": "^4.0.5",
      "yargs-parser": "^21.1.1"
    }
  }
}
