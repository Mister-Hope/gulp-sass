{
  "name": "@mr-hope/gulp-sass",
  "version": "3.1.1",
  "description": "Gulp plugin for sass",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "changelog": "standard-version --skip.bump --skip.commit --skip.tag",
    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "packages:check-update": "pnpm dlx npm-check-updates -u --timeout 600000",
    "packages:update": "pnpm up -r",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mister-Hope/gulp-sass"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gulp-plugin",
    "sass",
    "gulp"
  ],
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/gulp-sass/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,yaml,yml,md}": "prettier --write",
    "*.wxss": "stylelint --fix",
    "*.wxml": "prettier --parser html --write",
    "*.wxs": "prettier --parser babel-flow --write",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@types/node": "*",
    "picocolors": "^1.0.0",
    "plugin-error": "^2.0.1",
    "replace-ext": "^2.0.0",
    "sass": ">= 1.45.0",
    "source-map-js": "^1.0.2",
    "strip-ansi": "^6.0.1",
    "vinyl": "^2.2.1",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@types/globule": "1.1.5",
    "@types/gulp": "4.0.10",
    "@types/gulp-postcss": "8.0.2",
    "@types/gulp-sourcemaps": "0.0.35",
    "@types/gulp-tap": "1.0.1",
    "@types/rimraf": "3.0.2",
    "@types/vinyl": "2.0.7",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "@vitest/coverage-c8": "0.28.5",
    "autoprefixer": "10.4.13",
    "commitizen": "4.3.0",
    "cz-git": "1.5.0",
    "del": "7.0.0",
    "esbuild": "0.17.8",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "globule": "1.3.4",
    "gulp": "4.0.2",
    "gulp-postcss": "9.0.1",
    "gulp-sourcemaps": "3.0.0",
    "gulp-tap": "2.0.0",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "rimraf": "4.4.0",
    "rollup": "3.17.1",
    "rollup-plugin-dts": "5.2.0",
    "rollup-plugin-esbuild": "5.0.0",
    "standard-version": "9.5.0",
    "typescript": "4.9.5",
    "vite": "4.1.2",
    "vitest": "0.28.5"
  },
  "packageManager": "pnpm@7.29.2",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "cacache": "^17.0.4",
      "chokidar": "3.5.1",
      "css": "npm:@adobe/css-tools@4.0.1",
      "glob-stream": "^7.0.0",
      "glob-parent": "^6.0.2",
      "micromatch": "^4.0.5",
      "yargs-parser": "^21.1.1"
    }
  }
}
